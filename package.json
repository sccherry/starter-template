{
  "name": "starter-template",
  "version": "1.0.0",
  "description": "A starter template for building static sites.",
  "main": "index.js",
  "scripts": {
    "build:site": "eleventy --config eleventy.config.js",
    "build:css": "postcss src/css -d dist/css",
    "build": "ELEVENTY_ENV=production npm run build:site && NODE_ENV=production npm run build:css",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run build:site -- --serve & npm run build:css -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sccherry/starter-template.git"
  },
  "author": "Steve Cherry",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sccherry/starter-template/issues"
  },
  "homepage": "https://github.com/sccherry/starter-template#readme",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "@11ty/eleventy-navigation": "^0.1.6",
    "eslint": "^7.7.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "html-minifier": "^4.0.0",
    "postcss": "^7.0.32",
    "postcss-cli": "^7.1.1",
    "postcss-csso": "^4.0.0",
    "postcss-import": "^12.0.1",
    "postcss-normalize": "^9.0.0",
    "postcss-preset-env": "^6.7.0",
    "stylelint": "^13.6.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "tailwindcss": "^1.7.4"
  }
}
